<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
<g:DockLayoutPanel unit='EM'>
  <g:north size='5'>
    <g:Label ui:field="topLabel">Top</g:Label>
  </g:north>
  <g:center>
    <g:HTMLPanel>
      <g:Label ui:field="bodyLabel">Body</g:Label>
      <g:Button ui:field="loadButton" />
    </g:HTMLPanel>
  </g:center>
  <g:west size='10'>
      <g:VerticalPanel ui:field="sidePanel">
      </g:VerticalPanel>
  </g:west>
</g:DockLayoutPanel>
</ui:UiBinder> 
