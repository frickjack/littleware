<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
    <application>
         <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>en</supported-locale>
            <supported-locale>zh</supported-locale>
         </locale-config>
    </application>

    <navigation-rule>
        <from-view-id>/index.jsp</from-view-id>
        <navigation-case>
            <from-outcome>OK</from-outcome>
            <to-view-id>/welcome.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>FAILED</from-outcome>
            <to-view-id>/sorry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>REMOTE_DOWN</from-outcome>
            <to-view-id>/sorry.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lib/xsl/dynamic_layout.jsp</from-view-id>
        <navigation-case>
            <from-outcome>OK</from-outcome>
            <to-view-id>/en/home/home.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>FAILED</from-outcome>
            <to-view-id>/en/account/login.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>REMOTE_DOWN</from-outcome>
            <to-view-id>/en/account/login.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>LOGOUT</from-outcome>
            <to-view-id>/en/account/logout.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

